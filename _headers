# 自定义目录列表页面

# 移动端预加载
/CSS/custom/M_Preload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

/scripts/custom/M_Preload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

# 移动端后加载
/CSS/custom/M_Postload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

/scripts/custom/M_Postload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

# 桌面端预加载
/CSS/custom/Preload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

/scripts/custom/Preload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

# 桌面端后加载
/CSS/custom/Postload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true

/scripts/custom/Postload/*
  Content-Type: text/html
  X-Custom-Directory-Listing: true